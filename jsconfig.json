{
  "compilerOptions": {
    "types": [
      "unplugin-icons/types/vue",
    ],
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "skipLibCheck":true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "vueCompilerOptions":{
    "plugins":[
    "@vue/language-plugin-pug"
    ]
  }
}
